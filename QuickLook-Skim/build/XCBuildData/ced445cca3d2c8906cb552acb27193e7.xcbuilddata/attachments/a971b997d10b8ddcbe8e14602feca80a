#!/bin/sh
#
# Run the genstrings build phase
# This takes a while to generate, so it's not worth
# the wait for development builds.
#
if [ "${BUILD_STYLE}" = "Debug" -o "${CONFIGURATION}" = "Debug" ]; then
exit 0
fi
#
# Run the genstrings build phase
#
cd "${SOURCE_ROOT}"

/usr/bin/genstrings -q -o en.lproj *.m 2>&1 | sed -e 's/^/warning: /g' | sed -e 's/warning:.*Warning:/warning:/g' >&2

